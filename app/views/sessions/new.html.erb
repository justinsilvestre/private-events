<h1>Log in</h1>
<%= form_for(:session, url: login_path) do |f| %>
	
	<%= f.label :email %>
	<%= f.email_field :email %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.label :remember_me, "Remember me on this computer" %>
	<%= f.check_box :remember_me %>

	<%= f.submit "Log in" %>

<% end %>